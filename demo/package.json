{
  "private": true,
  "name": "demo",
  "version": "4.9.1",
  "dependencies": {
    "@apollo/client": "3.3.7",
    "@apollo/react-hooks": "4.0.0",
    "@types/node": "14.14.21",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "graphql-codegen-hasura-core": "^4.9.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@graphql-codegen/typescript-resolvers": "1.18.1",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "graphql": "^15.4.0",
    "graphql-codegen-hasura-client-config": "file:.yalc/graphql-codegen-hasura-client-config",
    "graphql-codegen-hasura-gql": "file:.yalc/graphql-codegen-hasura-gql",
    "graphql-codegen-hasura-react": "file:.yalc/graphql-codegen-hasura-react",
    "graphql-codegen-hasura-shared": "file:.yalc/graphql-codegen-hasura-shared",
    "graphql-codegen-hasura-typescript": "file:.yalc/graphql-codegen-hasura-typescript",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "type-fest": "^0.13.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen --config=graphql-codegen-gql.yaml; graphql-codegen --config=graphql-codegen-typescript.yaml",
    "build-and-generate": "lerna run build && npm run generate",
    "rebuild-and-generate": "lerna run rebuild && npm run generate",
    "bootstrap": "lerna bootstrap"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(aapi)"
    ]
  }
}
