    "@types/jest": "^25.2.3",
    "babel-jest": "^26.0.1",
    "graphql": "^15.0.0",
    "jest": "^26.0.1",
    "path": "^0.12.7",
    "tslib": "2.0.0",
    "typescript": "3.9.3",
